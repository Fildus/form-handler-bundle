{
    "name": "tboileau/form-handler-bundle",
    "description": "Symfony Form Handler helps you handle forms, in accordance with SOLID principles.",
    "keywords": ["form", "handler", "form handler", "handling forms"],
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.0.8",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/yaml": "^4.3@dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4|^4.0",
        "symfony/maker-bundle": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Boileau",
            "email": "t-boileau@email.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TBoileau\\Bundle\\FormHandlerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBoileau\\Bundle\\FormHandlerBundle\\Tests\\": "tests/"
        }
    }
}
